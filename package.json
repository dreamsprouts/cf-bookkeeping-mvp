{
  "name": "cf-bookkeeping-mvp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "test:e2e": "node scripts/e2e-webhook.mjs",
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "deploy:frontend": "wrangler pages deploy frontend --project-name=cf-bookkeeping-mvp",
    "db:create": "wrangler d1 create cf-bookkeeping-db",
    "db:migrate": "wrangler d1 execute cf-bookkeeping-db --remote --file=./db/schema.sql",
"db:migrate:local": "wrangler d1 execute cf-bookkeeping-db --local --file=./db/schema.sql",
    "db:migrate:extend": "wrangler d1 execute cf-bookkeeping-db --remote --file=./db/migrations/002_extend_entries.sql",
    "db:migrate:extend:local": "wrangler d1 execute cf-bookkeeping-db --local --file=./db/migrations/002_extend_entries.sql"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241127.0",
    "typescript": "^5.3.0",
    "wrangler": "^3.91.0"
  },
  "dependencies": {
    "hono": "^4.6.0"
  }
}
